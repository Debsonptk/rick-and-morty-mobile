{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "config/*": [".src/config/*"],
      "contexts/*": ["./src/contexts/*"],
      "env/*": ["./src/env/*"],
      "helpers/*": ["./src/helpers/*"],
      "hooks/*": ["./src/hooks/*"],
      "routes/*": ["./src/routes/*"],
      "screens/*": ["./src/screens/*"],
      "services/*": ["./src/services/*"],
      "theme/*": ["./src/theme/*"],
      "types/*": ["./src/types/*"]
    }
  }
}
